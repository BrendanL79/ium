{
  "images": [
    {
      "image": "linuxserver/calibre",
      "base_tag": "latest",
      "regex": "v[0-9]+\\.[0-9]+\\.[0-9]+-ls[0-9]+",
      "auto_update": true,
      "container_name": "calibre"
    },
    {
      "image": "linuxserver/sonarr",
      "base_tag": "latest",
      "regex": "[0-9]+\\.[0-9]+\\.[0-9]+\\.[0-9]+-ls[0-9]+",
      "auto_update": true,
      "container_name": "sonarr"
    },
    {
      "image": "linuxserver/plex",
      "base_tag": "latest",
      "regex": "[0-9]+\\.[0-9]+\\.[0-9]+\\.[0-9]+-ls[0-9]+",
      "auto_update": false,
      "container_name": "plex"
    },
    {
      "image": "postgres",
      "base_tag": "15",
      "regex": "15\\.[0-9]+-alpine",
      "auto_update": false,
      "container_name": "postgres"
    },
    {
      "image": "nginx",
      "base_tag": "mainline",
      "regex": "[0-9]+\\.[0-9]+\\.[0-9]+",
      "auto_update": true,
      "container_name": "nginx"
    },
    {
      "image": "node",
      "base_tag": "lts",
      "regex": "[0-9]+\\.[0-9]+\\.[0-9]+-alpine",
      "auto_update": false,
      "container_name": "node-app"
    },
    {
      "image": "redis",
      "base_tag": "7",
      "regex": "7\\.[0-9]+\\.[0-9]+-alpine",
      "auto_update": false,
      "container_name": "redis"
    },
    {
      "image": "mariadb",
      "base_tag": "11",
      "regex": "11\\.[0-9]+\\.[0-9]+",
      "auto_update": false,
      "container_name": "mariadb"
    },
    {
      "image": "homeassistant/home-assistant",
      "base_tag": "stable",
      "regex": "[0-9]{4}\\.[0-9]+\\.[0-9]+",
      "auto_update": false,
      "container_name": "homeassistant"
    },
    {
      "image": "grafana/grafana",
      "base_tag": "latest",
      "regex": "[0-9]+\\.[0-9]+\\.[0-9]+",
      "auto_update": false,
      "container_name": "grafana"
    },
    {
      "image": "influxdb",
      "base_tag": "2",
      "regex": "2\\.[0-9]+-alpine",
      "auto_update": false,
      "container_name": "influxdb"
    }
  ],
  "notifications": {
    "enabled": false,
    "webhook_url": "https://hooks.slack.com/services/YOUR/WEBHOOK/URL",
    "email": {
      "enabled": false,
      "smtp_server": "smtp.gmail.com",
      "smtp_port": 587,
      "from": "your-email@gmail.com",
      "to": "notify@example.com"
    }
  },
  "update_schedule": {
    "enabled": true,
    "check_interval_seconds": 3600,
    "update_window": {
      "start_hour": 2,
      "end_hour": 5
    }
  }
}

